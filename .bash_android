OS=$(uname)
case $OS in
	'Darwin')
		OS='osx'
		;;
	'Linux')
		OS='linux'
		;;
	*)
		echo "Unknown OS!"
		usage
esac

do_common_prefix(){
	:
}

do_osx(){
	:
	alias xlogout="osascript -e 'tell application \"System Events\" to log out'"
	alias xup="sudo softwareupdate -i -a"
}

do_linux(){
	:
}

do_common_postfix(){
	:
	source ~/my/app/android/envrc
	export PATH=$PATH:$APP/bin
}

do_common_prefix
do_$OS $*
do_common_postfix
